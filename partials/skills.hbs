{{#if resume.skills.length}}
<div id="skills" class="box">
<h2><i class="fas fa-palette ico"></i> Skills</h2>
{{#each resume.skills}}
<div class="skills clearfix">
	<div class="item-skills">
		{{name}}
		{{#if level}}<span class="skill-level">{{level}}</span>{{/if}}
	</div>
	<div class="col-sm-offset-1 col-sm-12 clearfix">
	{{#each keywords}}
	<span class= "skill badge">{{.}}</span>
	{{/each}}
	</div>
</div>
{{/each}}
</div>
{{/if}}